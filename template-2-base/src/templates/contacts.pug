extends partials/layout

//- Override default config variables
append config
  - var pageTitle = 'Contacts'
  - var parentMenu = 'Pages'
  - var menuTitle = 'Contacts'
  - var isGoogleMap = true
  - var breadcrumbs = [{'title': 'Home', 'link': 'index.html'}, {'title': 'Contacts'}]

