      .col-md-7
        .display-3.text-muted.opacity-75.mb-30 Customer Service
      .col-md-5
        ul.list-icon
          li 
            i.material-icons.mail_outline.text-muted
            a(href='mailto:customer.service@unishop.com').navi-link customer.service@unishop.com
          li 
            i.material-icons.phone.text-muted
            | +1 (080) 44 357 260
          li 
            i.material-icons.access_time.text-muted
            | 1 - 2 business days
    hr.margin-top-2x
    .row.margin-top-2x
      .col-md-7
        .display-3.text-muted.opacity-75.mb-30 Tech Support
      .col-md-5
        ul.list-icon
          li 
            i.material-icons.mail_outline.text-muted
            a(href='mailto:support@unishop.com').navi-link support@unishop.com
